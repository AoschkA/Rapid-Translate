{
  "name": "Rapid-Translate",
  "version": "6.0.0",
  "repository": "https://github.com/AoschkA/Rapid-Translate",
  "description": "A plugin for rapid transformation",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "Rapid-Translate",
    "manifest": "src/manifest.json",
    "main": "plugin.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
